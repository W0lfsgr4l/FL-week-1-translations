# game/code/d07/d07s04dnd.rpy:27
translate french d07s04dnd_0e144b2e:

    # dw "Yeah, hi [mcname]. What brings you to our side of the beach."
    dw "Ouais, salut [mcname]. Qu'est ce qui t'ammène sur notre partie de la plage."

# game/code/d07/d07s04dnd.rpy:28
translate french d07s04dnd_d4dbeae7:

    # mc "A jet-ski, actually."
    mc "Un jet-ski, à vrai dire."

# game/code/d07/d07s04dnd.rpy:30
translate french d07s04dnd_9cd9e72f:

    # dd "Oh? Did it sink?"
    dd "Oh? Il a coulé?"

# game/code/d07/d07s04dnd.rpy:31
translate french d07s04dnd_e44cff38:

